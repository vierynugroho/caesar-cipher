{
	"name": "classic-cryptography",
	"version": "1.1.1",
	"main": "dist/cjs/index.cjs.js",
	"module": "dist/esm/index.esm.js",
	"exports": {
		"require": "./dist/cjs/index.cjs.js",
		"import": "./dist/esm/index.esm.js"
	},
	"type": "module",
	"scripts": {
		"build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs",
		"build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm",
		"build": "npm run build:cjs && npm run build:esm",
		"start": "node src/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vierynugroho/team_jago-criptography.git"
	},
	"keywords": [
		"cryptography",
		"exhaustive",
		"key",
		"search",
		"caesar",
		"cipher",
		"vnonymous"
	],
	"author": "Viery Nugroho, vnonymous",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/vierynugroho/team_jago-criptography/issues"
	},
	"homepage": "https://github.com/vierynugroho/team_jago-criptography#readme",
	"devDependencies": {
		"@babel/cli": "^7.25.9",
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0"
	},
	"dependencies": {
		"cross-env": "^7.0.3"
	}
}
